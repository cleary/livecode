-- (initial) restriction: superfm only, 1 orbit
-- references:
--   https://www.youtube.com/watch?v=REgE33Esy2Q
--   https://club.tidalcycles.org/t/superfm/1761/16
--   https://club.tidalcycles.org/t/superfm-tutorial-live-stream/2358/5

setcps (125/240)

let pat = "<t(3,8) t*2> t"
in
d1  $ foldEvery [3,4] (# feedback 1) 
    $ foldEvery [3,4] (jux (# octave 7))
    $ off "<0.125!3 1/3 >" ((# squiz 2) . (# gain 0.7) . (# legato (choose [0.1, 0.5, 2, 1])) ) 
    $ slow 4 
    $ struct pat 
    $ note "<[fs3 e3 d3] g2> <c4'maj7 c3'maj7'iii>" 
    # s "superfm"
    # amp1 1
    # amp2 0
    # amp3 (2/3)
    # amp4 0
    # amp5 1
    # amp6 0
    # egrate11 "< 10 1>"
    # egrate12 10
    # egrate13 10
    # eglevel11 1
    # eglevel12 0
    # eglevel13 0
    # eglevel14 1.5
    # ratio3 2
    # egrate31 1
    # egrate32 10
    # egrate33 10
    # eglevel31 1
    # eglevel32 0
    # eglevel33 0
    # eglevel34 1.5
    # ratio5 4
    # mod13 "< 1 0 2 0 8>"
    # voice "1 3 2 4"

